    "symfony/twig-bundle": "5.4.*",
    "symfony/uid": "5.4.*",
    "symfony/validator": "5.4.*",
    "symfony/yaml": "5.4.*"
  },
  "config": {
    "allow-plugins": {
      "composer/package-versions-deprecated": true,
      "symfony/flex": true,
      "symfony/runtime": true,
      "php-http/discovery": true
    },
    "optimize-autoloader": true,
    "preferred-install": {
      "*": "dist"
    },
    "sort-packages": true
  },
  "autoload": {
    "psr-4": {
      "App\\": "src/",
      "Wise\\Core\\": "Wise/Core/",
      "Wise\\Cart\\": "Wise/Cart/",
      "Wise\\Order\\": "Wise/Order/",
      "Wise\\OrderEdit\\": "Wise/OrderEdit/",
      "Wise\\File\\": "Wise/File/",
      "Wise\\Pricing\\": "Wise/Pricing/",
      "Wise\\Product\\": "Wise/Product/",
      "Wise\\Receiver\\": "Wise/Receiver/",
      "Wise\\Client\\": "Wise/Client/",
      "Wise\\ClientApi\\": "Wise/ClientApi/",
      "Wise\\Cms\\": "Wise/Cms/",
      "Wise\\Stock\\": "Wise/Stock/",
      "Wise\\Delivery\\": "Wise/Delivery/",
      "Wise\\SearchProduct\\": "Wise/SearchProduct/",
      "Wise\\Document\\": "Wise/Document/",
      "Wise\\Payment\\": "Wise/Payment/",
      "Wise\\Checkout\\": "Wise/Checkout/",
      "Wise\\Security\\": "Wise/Security/",
      "Wise\\Service\\": "Wise/Service/",
      "Wise\\I18n\\": "Wise/I18n/",
      "Wise\\User\\": "Wise/User/",
      "Wise\\Message\\": "Wise/Message/",
      "Wise\\Offer\\": "Wise/Offer/",
      "Wise\\ShoppingList\\": "Wise/ShoppingList/",
      "Wise\\ExportCatalog\\": "Wise/ExportCatalog/",
      "Wise\\DynamicUI\\": "Wise/DynamicUI/",
      "Wise\\MultiStore\\": "Wise/MultiStore/",
      "Wise\\Agreement\\": "Wise/Agreement/",
      "Wise\\GPSR\\": "Wise/GPSR/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "App\\Tests\\": "tests/",
      "Wise\\Core\\Tests\\": "Wise/Core/Tests/",
      "Wise\\Cart\\Tests\\": "Wise/Cart/Tests/",
      "Wise\\Order\\Tests\\": "Wise/Order/Tests/",
      "Wise\\File\\Tests\\": "Wise/File/Tests/",
      "Wise\\Pricing\\Tests\\": "Wise/Pricing/Tests/",
      "Wise\\Product\\Tests\\": "Wise/Product/Tests/",
      "Wise\\Receiver\\Tests\\": "Wise/Receiver/Tests/",
      "Wise\\Client\\Tests\\": "Wise/Client/Tests/",
      "Wise\\ClientApi\\Tests\\": "Wise/ClientApi/Tests/",
      "Wise\\Cms\\Tests\\": "Wise/Cms/Tests/",
      "Wise\\Stock\\Tests\\": "Wise/Stock/Tests/",
      "Wise\\Delivery\\Tests\\": "Wise/Delivery/Tests/",
      "Wise\\SearchProduct\\Tests\\": "Wise/SearchProduct/Tests/",
      "Wise\\Document\\Tests\\": "Wise/Document/Tests/",
      "Wise\\Payment\\Tests\\": "Wise/Payment/Tests/",
      "Wise\\Checkout\\Tests\\": "Wise/Checkout/Tests/",
      "Wise\\Security\\Tests\\": "Wise/Security/Tests/",
      "Wise\\Service\\Tests\\": "Wise/Service/Tests/",
      "Wise\\I18n\\Tests\\": "Wise/I18n/Tests/",
      "Wise\\User\\Tests\\": "Wise/User/Tests/",
      "Wise\\Message\\Tests\\": "Wise/Message/Tests/",
      "Wise\\Offer\\Tests\\": "Wise/Offer/Tests/",
      "Wise\\ShoppingList\\Tests\\": "Wise/ShoppingList/Tests/",
      "Wise\\ExportCatalog\\Tests\\": "Wise/ExportCatalog/Tests/",
      "Wise\\MultiStore\\Tests\\": "Wise/MultiStore/Tests/"
    }
  },
  "replace": {
    "symfony/polyfill-ctype": "*",
    "symfony/polyfill-iconv": "*",
    "symfony/polyfill-php72": "*"
  },
  "scripts": {
    "auto-scripts": {
      "vendor/bin/codecept build": "php-script",
      "cache:clear": "symfony-cmd",
      "assets:install %PUBLIC_DIR%": "symfony-cmd"
    },
    "set-permissions": [
      "php -r \"// Ustawianie uprawnien do katalogow\"",
      "php -r \"@chmod( __DIR__ . '/public/files/article_field_file', 0775);\"",
      "php -r \"@chmod( __DIR__ . '/public/files/document', 0775);\"",
      "php -r \"@chmod( __DIR__ . '/public/files/images', 0775);\"",
      "php -r \"@chmod( __DIR__ . '/public/files/orders', 0775);\"",
      "php -r \"@chmod( __DIR__ . '/public/files/products', 0775);\"",
      "php -r \"@chmod( __DIR__ . '/public/files/categories', 0775);\"",
      "php -r \"@chmod( __DIR__ . '/var/cache/prod', 0775);\"",
      "php -r \"@chmod( __DIR__ . '/var/cache/dev', 0775);\"",
      "php -r \"@chmod( __DIR__ . '/var/log', 0775);\"",
      "php -r \"@chgrp( __DIR__ . '/public/files/article_field_file', 'www-data');\"",
      "php -r \"@chgrp( __DIR__ . '/public/files/document', 'www-data');\"",
      "php -r \"@chgrp( __DIR__ . '/public/files/images', 'www-data');\"",
      "php -r \"@chgrp( __DIR__ . '/public/files/orders', 'www-data');\"",
      "php -r \"@chgrp( __DIR__ . '/public/files/products', 'www-data');\"",
      "php -r \"@chgrp( __DIR__ . '/public/files/categories', 'www-data');\"",
      "php -r \"@chgrp( __DIR__ . '/var/cache/prod', 'www-data');\"",
      "php -r \"@chgrp( __DIR__ . '/var/cache/dev', 'www-data');\"",
      "php -r \"@chgrp( __DIR__ . '/var/log', 'www-data');\""
    ],
    "post-install-cmd": [
      "@auto-scripts",
      "@set-permissions"
    ],
    "post-update-cmd": [
      "@auto-scripts",
      "@set-permissions"
    ]
  },
  "conflict": {
    "symfony/symfony": "*"
  },
  "extra": {
    "symfony": {
      "allow-contrib": false,
      "require": "5.4.*"
    }
  },
  "require-dev": {
    "codeception/codeception": "^5.0",
    "codeception/module-asserts": "*",
    "codeception/module-doctrine2": "*",
    "codeception/module-phpbrowser": "*",
    "codeception/module-rest": "*",
    "codeception/module-symfony": "*",
    "doctrine/doctrine-fixtures-bundle": "^3.4",
    "fakerphp/faker": "^1.23",
    "symfony/process": "5.4.*",
    "symfony/stopwatch": "5.4.*",
    "symfony/web-profiler-bundle": "5.4.*"
  }
}
