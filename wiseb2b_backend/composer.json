    "phpoffice/phpspreadsheet": "^1.28",
    "phpstan/phpdoc-parser": "^1.29",
    "sensio/framework-extra-bundle": "^6.2",
    "sentry/sentry-symfony": "^4.10",
    "simplehtmldom/simplehtmldom": "2.0-RC2",
    "symfony/amqp-messenger": "5.4.*",
    "symfony/asset": "5.4.*",
